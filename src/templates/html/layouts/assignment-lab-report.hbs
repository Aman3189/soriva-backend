    .observation-table-wrapper {
      overflow-x: auto;
      margin: 0.75rem 0;
    }

    .observation-table {
      width: 100%;
      border-collapse: collapse;
      font-size: 0.9rem;
    }

    .observation-table caption {
      font-weight: 600;
      color: var(--primary-color);
      margin-bottom: 0.5rem;
      text-align: left;
      font-size: 0.95rem;
    }

    .observation-table th {
      background: var(--primary-color);
      color: #fff;
