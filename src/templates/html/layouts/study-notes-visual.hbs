    .visual-key-point:nth-child(6n+2) .key-point-icon { background: var(--diagram-green); }
    .visual-key-point:nth-child(6n+3) .key-point-icon { background: var(--diagram-orange); }
    .visual-key-point:nth-child(6n+4) .key-point-icon { background: var(--diagram-purple); }
    .visual-key-point:nth-child(6n+5) .key-point-icon { background: var(--diagram-pink); }
    .visual-key-point:nth-child(6n+6) .key-point-icon { background: var(--diagram-teal); }

