      {{#if scope}}
      <h3 style="font-size: 1.05rem; font-weight: 600; margin: 1.25rem 0 0.75rem; color: var(--primary-color);">1.3 Scope</h3>
      <p>{{scope}}</p>
      {{/if}}
    </div>
  </section>
  {{/if}}

