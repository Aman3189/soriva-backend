      border-radius: 25px;
      font-size: 0.8rem;
      font-weight: 600;
      text-transform: uppercase;
      letter-spacing: 1px;
      margin-bottom: 1.5rem;
    }

    .cover-title {
      font-size: 1.5rem;
      font-weight: 700;
      color: var(--primary-color);
      margin-bottom: 0.5rem;
      line-height: 1.3;
    }

    .cover-subtitle {
      font-size: 1rem;
      color: var(--text-light);
      margin-bottom: 2rem;
    }

    .company-badge {
      background: var(--bg-green);
      border: 1px solid #a7f3d0;
      border-radius: 12px;
      padding: 1.25rem 2rem;
      margin-bottom: 2rem;
    }

    .company-badge .at-label {
