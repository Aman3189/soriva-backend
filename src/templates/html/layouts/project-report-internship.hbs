      display: flex;
      flex-direction: column;
      border: 3px solid var(--primary-color);
      position: relative;
    }

    .cover-header {
      background: linear-gradient(135deg, var(--primary-color) 0%, var(--secondary-color) 100%);
      color: #fff;
      padding: 1.5rem;
      text-align: center;
    }

    .cover-header .inst-logo {
      width: 70px;
      height: 70px;
      margin: 0 auto 0.75rem;
      background: rgba(255,255,255,0.15);
      border-radius: 12px;
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .cover-header .inst-logo img {
      width: 55px;
      height: 55px;
      object-fit: contain;
    }

    .cover-header .inst-name {
      font-size: 1.2rem;
      font-weight: 700;
      text-transform: uppercase;
      letter-spacing: 1px;
    }

    .cover-header .dept-name {
