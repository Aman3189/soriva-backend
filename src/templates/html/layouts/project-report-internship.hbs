      color: var(--text-light);
    }

    .student-info-box {
      background: var(--bg-light);
      border: 1px solid var(--border-color);
      border-radius: 12px;
      padding: 1.25rem 2rem;
      margin-bottom: 1.5rem;
    }

    .student-info-box .student-label {
      font-size: 0.7rem;
      text-transform: uppercase;
      letter-spacing: 1.5px;
      color: var(--text-muted);
      margin-bottom: 0.5rem;
    }

    .student-info-box .student-name {
      font-size: 1.15rem;
      font-weight: 600;
    }

    .student-info-box .student-details {
      font-size: 0.9rem;
      color: var(--text-light);
    }

    .duration-box {
      display: flex;
      gap: 2rem;
      margin-top: 1rem;
    }

    .duration-item {
      text-align: center;
    }

    .duration-item .dur-label {
      font-size: 0.7rem;
      text-transform: uppercase;
      letter-spacing: 1px;
      color: var(--text-muted);
